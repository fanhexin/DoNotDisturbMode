	-$(INSTALL_FILE) d:/studio/Github/DoNotDisturbMode/DoNotDisturb/zh_CN.ts $(INSTALL_ROOT)/opt/DoNotDisturb/lang/
	-$(INSTALL_FILE) d:/studio/Github/DoNotDisturbMode/DoNotDisturb/zh_CN.qm $(INSTALL_ROOT)/opt/DoNotDisturb/lang/
	-$(INSTALL_FILE) d:/studio/Github/DoNotDisturbMode/DoNotDisturb/zh_HK.ts $(INSTALL_ROOT)/opt/DoNotDisturb/lang/
	-$(INSTALL_FILE) d:/studio/Github/DoNotDisturbMode/DoNotDisturb/zh_HK.qm $(INSTALL_ROOT)/opt/DoNotDisturb/lang/
	-$(INSTALL_FILE) d:/studio/Github/DoNotDisturbMode/DoNotDisturb/zh_TW.ts $(INSTALL_ROOT)/opt/DoNotDisturb/lang/
	-$(INSTALL_FILE) d:/studio/Github/DoNotDisturbMode/DoNotDisturb/zh_TW.qm $(INSTALL_ROOT)/opt/DoNotDisturb/lang/


uninstall_translation:  FORCE
	-$(DEL_FILE) -r $(INSTALL_ROOT)/opt/DoNotDisturb/lang/zh_CN.ts 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/opt/DoNotDisturb/lang/zh_CN.qm 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/opt/DoNotDisturb/lang/zh_HK.ts 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/opt/DoNotDisturb/lang/zh_HK.qm 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/opt/DoNotDisturb/lang/zh_TW.ts 
	 -$(DEL_FILE) -r $(INSTALL_ROOT)/opt/DoNotDisturb/lang/zh_TW.qm
	-$(DEL_DIR) $(INSTALL_ROOT)/opt/DoNotDisturb/lang/ 


install:  install_itemfolder_01 install_icon install_desktopfile install_target install_translation  FORCE

uninstall: uninstall_itemfolder_01 uninstall_icon uninstall_desktopfile uninstall_target uninstall_translation   FORCE

FORCE:

